<h3> Bookings for <%= @store_bike.bike.name %>   
   <%= @store_bike.store.name %> </h3>
<table class="bookings_table">
  <tr>
    <th>Start</th>
    <th>End</th>
  </tr>
  
  <% @bookings.each do |booking| %>
  <tr>
    <td>
      <%= booking.start_time.strftime('%e %b %Y %H:%M%p') %>
    </td>
    <td>
      <%= (booking.calculate_end_time + 60).strftime('%e %b %Y %H:%M%p') %>
    </td>
    <td><%= link_to "Delete", store_bike_booking_path(@store_bike, booking), data: { confirm: 'Are you sure?' }, method: :delete %>
    </td>
    <td><%= link_to "Edit", edit_store_bike_booking_path(@store_bike, booking) %>
    </td>
  </tr>
  <% end %>
</table>